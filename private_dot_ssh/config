# Hackintosh virtual machine
Host macos
    HostName localhost
    Port 22220
    User nchiang
    IdentityFile ~/.ssh/id_ed25519
    ForwardAgent yes

# Stanford SING research group lab machine
Host watchog
    HostName watchog.stanford.edu
    User nchiang
    ProxyJump sing.stanford.edu
    LocalForward 8890 127.0.0.1:8890

